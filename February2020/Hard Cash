#include <bits/stdc++.h> 
using namespace std;
#define ll long long
main() 
{ ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
ll t,n,k,i; cin>>t;
while(t--){
    ll c=0;
    cin>>n>>k;
    ll a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
        c+=a[i]%k;
    }
    cout<<c%k<<endl;
}
}
